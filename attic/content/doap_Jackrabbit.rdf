        <browse rdf:resource="http://svn.apache.org/viewvc/jackrabbit/trunk/"/>
        <location rdf:resource="http://svn.apache.org/repos/asf/jackrabbit/trunk/"/>
      </SVNRepository>
    </repository>

    <mailing-list rdf:resource="http://jackrabbit.apache.org/mail-lists.html"/>

